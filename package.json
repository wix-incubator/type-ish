{
  "name": "type-ish",
  "version": "0.1.5",
  "description": "JavaScript Runtime Contract Validations",
  "license": "MIT",
  "nativePackage": true,
  "author": "Daniel Zlotin <zlotindaniel@gmail.com>",
  "contributors": [
    "Tal Kol <talkol@gmail.com>"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/wix/type-ish/issues"
  },
  "homepage": "https://github.com/wix/type-ish",
  "readme": "https://github.com/wix/type-ish#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/type-ish.git"
  },
  "main": "src/index.js",
  "scripts": {
    "clean": ":",
    "build": ":",
    "test": "xo && jest --coverage",
    "prerelease": "npm run test",
    "release": "npm version patch && git push && npm publish"
  },
  "dependencies": {
    "lodash": "4.x.x"
  },
  "devDependencies": {
    "xo": "0.18.x",
    "jest": "20.x.x"
  },
  "jest": {
    "roots": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/"
    ],
    "resetMocks": true,
    "resetModules": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "xo": {
    "space": true,
    "env": [
      "node",
      "jest"
    ],
    "rules": {
      "new-cap": 0,
      "unicorn/filename-case": 0,
      "arrow-parens": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ]
    }
  }
}
